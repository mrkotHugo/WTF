						["custom"] = "function (allstates, event, ...)\n    if aura_env.config.disableSharedNote and aura_env.config.disablePersonalNote then\n        return false\n    end\n    if not (aura_env.config.enableWNoteDisabled or aura_env.Note.enabled) then\n        return false\n    end\n    \n    if event == \"COMBAT_LOG_EVENT_UNFILTERED\" then\n        if not aura_env.cleucheck then\n            return false\n        end\n        \n        local _, opt, _, _, _, _, _, _, _, _, _, spellID, spellName = ...\n        \n        local thisEvent = aura_env.eventTable[opt]\n        if not thisEvent then\n            return false\n        end\n        local condition = thisEvent .. spellID\n        if aura_env.checks[condition] then\n            local encounterStarted = aura_env.encounterStarted \n            local auraid = aura_env.id\n            aura_env.checks[condition] = aura_env.checks[condition] + 1\n            DebugPrint(\"CLEU\", condition, spellName, aura_env.checks[condition])\n            local option_w_counter = thisEvent .. \":\" .. spellID .. \":\" .. aura_env.checks[condition]\n            local condition_w_counter = aura_env.conditions[option_w_counter]\n            if condition_w_counter then\n                for _, k in ipairs(condition_w_counter) do\n                    table.insert(\n                        aura_env.timers,\n                        C_Timer.NewTimer(\n                            k.start < 0 and 0.1 or k.start,\n                            function()\n                                WeakAuras.ScanEvents(\n                                    \"ERT_NOTE_NEW\",\n                                    k.start,\n                                    k.line,\n                                    nil,\n                                    encounterStarted,\n                                    auraid\n                                )\n                            end\n                        )\n                    )\n                    DebugPrint(\"New CLEU Timer w/ counter\", option_w_counter, k.line, k.start)\n                end\n            end\n            local option_wo_counter = thisEvent .. \":\" .. spellID .. \":\" .. 0\n            local condition_wo_counter = aura_env.conditions[option_wo_counter]\n            if condition_wo_counter then\n                for _, k in ipairs(condition_wo_counter) do\n                    table.insert(\n                        aura_env.timers,\n                        C_Timer.NewTimer(\n                            k.start < 0 and 0.1 or k.start,\n                            function()\n                                WeakAuras.ScanEvents(\n                                    \"ERT_NOTE_NEW\",\n                                    k.start,\n                                    k.line,\n                                    nil,\n                                    encounterStarted,\n                                    auraid\n                                )\n                            end\n                        )\n                    )\n                    DebugPrint(\"New CLEU Timer w/o counter\", option_wo_counter, k.line, k.start)\n                end\n            end\n        end\n    elseif event == \"ERT_NOTE_PHASE\"\n    and (IsAddOnLoaded(\"ExRT\") or IsAddOnLoaded(\"MRT\"))\n    and ...\n    then\n        local opt, opt2 = ...\n        local opts = opt .. opt2\n        if not aura_env.checks[opts] or opt2 == aura_env.phase then\n            return false\n        end\n        DebugPrint(\"New Phase\", opt2)\n        aura_env.checks[opts] = aura_env.checks[opts] + 1\n        aura_env.phase = opt2\n        local conditionPhase = aura_env.conditions[opts]\n        if conditionPhase then\n            local encounterStarted = aura_env.encounterStarted \n            local auraid = aura_env.id\n            for _, k in ipairs(conditionPhase) do\n                table.insert(\n                    aura_env.timers,\n                    C_Timer.NewTimer(\n                      