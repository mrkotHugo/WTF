					["custom"] = "--RaidFlags to markNum \naura_env.convert = {\n    [1] = 1,\n    [2] = 2,\n    [4] = 3,\n    [8] = 4,\n    [16] = 5,\n    [32] = 6,\n    [64] = 7,\n    [128] = 8,\n}\naura_env.split = function(str, pat)\n    local t = {}  -- NOTE: use {n = 0} in Lua-5.0\n    local fpat = \"(.-)\" .. pat\n    local last_end = 1\n    local s, e, cap = str:find(fpat, 1)\n    while s do\n        if s ~= 1 or cap ~= \"\" then\n            table.insert(t, cap)\n        end\n        last_end = e+1\n        s, e, cap = str:find(fpat, last_end)\n    end\n    if last_end <= #str then\n        cap = str:sub(last_end)\n        table.insert(t, cap)\n    end\n    return t\nend\naura_env.args = {\n    [\"SPELL_CAST_SUCCESS\"] = function(...)\n        local sourceGUID = select(4, ...)\n        local sourceRaidFlags =  select(7, ...)\n        local spellId = select(12, ...)\n        return sourceGUID, sourceRaidFlags > 0 and aura_env.convert[sourceRaidFlags] or 0, spellId\n    end,\n    [\"SPELL_INTERRUPT\"] = function(...)\n        local destGUID = select(8, ...)\n        local destRaidFlags =  select(11, ...)\n        local spellId = select(15, ...)\n        return destGUID, destRaidFlags > 0 and aura_env.convert[destRaidFlags] or 0, spellId\n    end,\n}\nlocal locals = {\n    --- Raid Target Icon [ENG]\n    {\n        \"star\",\n        \"circle\",\n        \"diamond\",\n        \"triangle\",\n        \"moon\",\n        \"square\",\n        \"cross\",\n        \"skull\",\n    },\n    --- Raid Target Icon [DE]\n    {\n        \"stern\",\n        \"kreis\",\n        \"diamant\",\n        \"dreieck\",\n        \"mond\",\n        \"quadrat\",\n        \"kreuz\",\n        \"totenschdel\",\n    },\n    --- Raid Target Icon [FR]\n    {\n        \"toile\",\n        \"cercle\",\n        \"losange\",\n        \"triangle\",\n        \"lune\",\n        \"carr\",\n        \"croix\",\n        \"crne\",\n    },\n    --- Raid Target Icon [IT]\n    {\n        \"stella\",\n        \"cerchio\",\n        \"rombo\",\n        \"triangolo\",\n        \"luna\",\n        \"quadrato\",\n        \"croce\",\n        \"teschio\",\n    },\n    --- Raid Target Icon [RU]\n    {\n        \"\",\n        \"\",\n        \"\",\n        \"\",\n        \"\",\n        \"\",\n        \"\",\n        \"\",\n    },\n    --- Raid Target Icon [ES]\n    {\n        \"dorado\",\n        \"naranja\",\n        \"morado\",\n        \"verde\",\n        \"plateado\",\n        \"azul\",\n        \"rojo\",\n        \"blanco\",\n    },\n    --- Raid Target Icon [PT]\n    {\n        \"dourado\",\n        \"laranja\",\n        \"roxo\",\n        \"verde\",\n        \"prateado\",\n        \"azul\",\n        \"vermelho\",\n        \"branco\",\n    }\n}\naura_env.toMarkNum = {}\nfor _, loc in ipairs(locals) do\n    for markNum, markName in ipairs(loc) do\n        aura_env.toMarkNum[markName] = markNum\n    end\nend\nfor markName, markNum in pairs(ICON_TAG_LIST) do\n    aura_env.toMarkNum[markName] = markNum\nend\naura_env.sound = {\n    [1] = \"None\",\n    [2] = \"Interface\\\\AddOns\\\\WeakAuras\\\\Media\\\\Sounds\\\\RingingPhone.ogg\",\n    [3] = \"Interface\\\\AddOns\\\\SharedMedia_Causese\\\\sound\\\\Next.ogg\",\n}\naura_env.castsound = {\n    [1] = \"None\",\n    [2] = \"Interface\\\\AddOns\\\\WeakAuras\\\\Media\\\\Sounds\\\\AirHorn.ogg\",\n    [3] = \"Interface\\\\AddOns\\\\WeakAuras\\\\Media\\\\Sounds\\\\BikeHorn.ogg\",\n    [4] = \"Interface\\\\AddOns\\\\SharedMedia_Causese\\\\sound\\\\Interrupt.ogg\",\n}\naura_env.sendemote = {\n    [1] = \"STAR\",\n    [2] = \"CIRCLE\",\n    [4] = \"DIAMOND\",\n    [8] = \"TRIANGLE\",\n    [16] = \"MOON\",\n    [32] = \"SQUARE\",\n    [64] = \"CROSS\",\n    [128] = \"SKULL\",\n}\naura_env.assignments = {}",
